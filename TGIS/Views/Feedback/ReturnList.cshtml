@model IEnumerable<TGIS.Models.Feedback>

@{
    ViewBag.Title = "ReturnList";
}

<h2>回報列表</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ReceivedDate)
        </th>
        <th>
            回報類型
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsRead)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ReceivedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tag.TagName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsRead)
        </td>
        <td>
            @Html.ActionLink("詳細", "ReturnDetail", new { id=item.ID }) |
            @Html.ActionLink("刪除", "ReturnDel", new { id=item.ID })
        </td>
    </tr>
}

</table>
